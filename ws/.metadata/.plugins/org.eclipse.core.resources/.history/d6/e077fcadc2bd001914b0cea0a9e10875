package practice.recursion;

public class Recursion {

	public static void main(String[] args) {
		printHello(5);

	}

	public static void printHello(int num) {
		System.out.println("Hello");
		num++;
		if(num < 20) {
		printHello(num);
		
	}
		
		for(int i = 0; i < 10 ; i++) {
			System.out.println("-------");
			printHello(num);
		}
}
}
